{
  "name": "Posters_Galore_Android",
  "version": "0.0.1",
  "engine": "node >= 4.2.2",
  "private": true,
  "scripts": {
    "test": "make test"
  },
  "jest": {
    "scriptPreprocessor": "jestSupport/scriptProcessor.js",
    "setupEnvScriptFile": "node_modules/react-native/jestSupport/env.js",
    "testPathIgnorePatterns": [
      "node_modules/",
      "__tests__/mocks/"
    ],
    "testFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map",
      "nock",
      "fetch",
      "react",
      "redux",
      "redux-thunk",
      "sinon",
      "src/",
      "__tests__/mocks"
    ]
  },
  "dependencies": {
    "react-native": "~0.14.2",
    "react-redux": "~3.1.0",
    "redux": "~3.0.4",
    "redux-thunk": "~1.0.0"
  },
  "devDependencies": {
    "babel-core": "~5.8.34",
    "es6-promise": "~3.0.2",
    "eslint": "~1.9.0",
    "eslint-config-airbnb": "~1.0.0",
    "eslint-plugin-mocha": "~1.0.0",
    "eslint-plugin-react": "~3.8.0",
    "isomorphic-fetch": "~2.2.0",
    "jest-cli": "0.5.4",
    "nock": "~2.17.0",
    "react": "~0.14.2",
    "react-addons-test-utils": "~0.14.2",
    "sinon": "~1.17.2"
  }
}
